{
    "name": "hrm-selenium-typescript",
    "version": "1.0.0",
    "description": "Selenium JS based test automation framework for HRM",
    "main": "index.js",
    "scripts": {
        "test": "mocha",
        "test:dev": "NODE_ENV=development mocha -r ts-node/register -r tsconfig-paths/register 'tests/specs/**/*.spec.ts' --reporter allure-mocha --reporter-options resultsDir=allure-results",
        "allure:generate": "allure generate allure-results --clean -o allure-report",
        "allure:open": "allure open allure-report",
        "test:stage": "NODE_ENV=stage mocha",
        "test:prod": "NODE_ENV=production mocha",
        "smoke": "mocha --grep '@smoke'",
        "smoke:dev": "NODE_ENV=development mocha --grep '@smoke'",
        "smoke:stage": "NODE_ENV=stage mocha --grep '@smoke'",
        "smoke:prod": "NODE_ENV=production mocha --grep '@smoke'",
        "docker:start": "docker compose -f docker-compose.yml up -d",
        "docker:start:arm": "docker compose -f docker-compose-arm.yml up -d",
        "docker:stop": "docker compose -f docker-compose.yml down",
        "docker:stop:arm": "docker compose -f docker-compose-arm.yml down",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "prepare": "husky"
    },
    "keywords": [
        "Selenium",
        "TypeScript",
        "MochaJs",
        "ChaiJs"
    ],
    "author": "Nghia Tran Minh",
    "license": "ISC",
    "devDependencies": {
        "@eslint/js": "^9.14.0",
        "@types/chai": "^4.3.20",
        "@types/mocha": "^9.1.1",
        "@types/node": "^22.9.0",
        "@types/selenium-webdriver": "^4.1.27",
        "allure-commandline": "^2.32.0",
        "allure-mocha": "^3.0.6",
        "chai": "^4.5.0",
        "dotenv": "^16.4.5",
        "eslint": "^9.14.0",
        "eslint-config-prettier": "^9.1.0",
        "fs": "^0.0.1-security",
        "globals": "^15.12.0",
        "husky": "^9.1.6",
        "mocha": "^10.8.2",
        "path": "^0.12.7",
        "selenium-webdriver": "^4.26.0",
        "ts-node": "^10.9.2",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^4.9.5",
        "typescript-eslint": "^8.14.0"
    },
    "dependencies": {
        "es-toolkit": "^1.27.0"
    }
}
